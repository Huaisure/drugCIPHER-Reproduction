    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"data_atc.csv\")\n",
    "df = df.drop_duplicates(subset=[\"dg_id\"])\n",
    "df.drop(columns=[\"Unnamed: 0\"], inplace=True)\n",
    "df = df[df[\"dg_atc_codes\"].apply(lambda x: x != \"[]\")].reset_index(\n",
    "        drop=True\n",
    "    )\n",
    "df.to_csv(\"data_atc.csv\",index=False)"
   ]
  }
